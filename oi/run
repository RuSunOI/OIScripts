go ()
{
    echo ---------------
    ./compile ./$1
    if [ $? -ne 0 ]
    then
        echo CE in $1
        return 0
    fi
    ./$1 < input > $1.out
    if [ $? -ne 0 ]
    then
        echo RE in $1
        return 0
    fi
    echo output for $1
    cat $1.out
}

if [ $# -eq 0 ]
then
    go test
fi

for i in $*
do
    go $i
done